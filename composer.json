{
    "name": "verbb/navigation",
    "description": "A Craft CMS plugin to create navigation menus for your site.",
    "type": "craft-plugin",
    "version": "1.0.13",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "navigation",
        "menu"
    ],
    "support": {
        "email": "support@verbb.io",
        "issues": "https://github.com/verbb/navigation/issues?state=open",
        "source": "https://github.com/verbb/navigation",
        "docs": "https://github.com/verbb/navigation",
        "rss": "https://github.com/verbb/navigation/commits/v2.atom"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Verbb",
            "homepage": "https://verbb.io"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "verbb\\navigation\\": "src/"
        }
    },
    "extra": {
        "name": "Navigation",
        "handle": "navigation",
        "schemaVersion": "1.0.3",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/verbb/navigation/master/CHANGELOG.md",
        "class": "verbb\\navigation\\Navigation"
    }
}
